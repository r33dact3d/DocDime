{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/bsv-price.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bsv-price",
      "dest": "api/bsv-price.js"
    },
    {
      "src": "/api/upload",
      "dest": "api/upload.js"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "PUBLIC_URL": "/"
  }
}