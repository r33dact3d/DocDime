{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "REACT_APP_HANDCASH_APP_ID": "@handcash_app_id",
    "REACT_APP_HANDCASH_APP_SECRET": "@handcash_app_secret"
  },
  "build": {
    "env": {
      "CI": "false"
    }
  }
}