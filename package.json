{
  "name": "docdime",
  "version": "1.0.0",
  "type": "commonjs",
  "private": true,
  
  "scripts": {
    "start": "react-scripts start",
    "start:server": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "vercel-build": "CI=false npm run build && echo 'Build completed successfully'",
    "prebuild": "echo 'Node version:' && node --version && echo 'NPM version:' && npm --version && echo 'Starting build process'",
    "postbuild": "echo 'Build process completed' && ls -la build",
    "deploy": "vercel",
    "vercel-login": "vercel login"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    } 
  }
}
